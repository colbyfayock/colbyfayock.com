    "fuse.js": "^6.6.2",
    "graphql": "^16.5.0",
    "he": "^1.2.0",
    "loader-utils": "^3.2.0",
    "next": "12.2.4",
    "parameterize": "^1.0.0",
    "path": "^0.12.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.4.0",
    "rss": "^1.2.2",
    "sass": "^1.54.3"
  },
  "devDependencies": {
    "eslint": "^8.21.0",
    "eslint-config-next": "^12.2.4",
    "eslint-config-prettier": "^8.5.0",
